<?xml version="1.0" encoding="UTF-8"?>
<ddm:DDM xmlns:ddm="http://easy.dans.knaw.nl/schemas/md/ddm/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:abr="http://www.den.nl/standaard/166/Archeologisch-Basisregister/"
    xsi:schemaLocation=
    "http://easy.dans.knaw.nl/schemas/md/ddm/ http://easy.dans.knaw.nl/schemas/md/2014/09/ddm.xsd
    http://www.den.nl/standaard/166/Archeologisch-Basisregister/ http://easy.dans.knaw.nl/schemas/vocab/2012/10/abr-type.xsd">
    
    <!-- If you view this example in a browser, click 'page source' to see the namespace declarations. -->
    
    <!-- 
    Example of the use of abr-types. Mark declaration of namespace and schemaLocation in the root element
    (see above).
    -->
    
    <ddm:dcmiMetadata>
        
        <!-- Use abr:ABRcomplex on dc:subject, dct:subject. For codes and their meaning see
        http://easy.dans.knaw.nl/schemas/docs/generated/abr-type/abr-type2.html#id7 -->
        <dc:subject xsi:type="abr:ABRcomplex">EGVU</dc:subject>
        <dc:subject xsi:type="abr:ABRcomplex">EIVB</dc:subject>
        
        <!-- Use abr:ABRperiode on dct:temporal. For codes and their meaning see
        http://easy.dans.knaw.nl/schemas/docs/generated/abr-type/abr-type2.html#id8 -->
        <dct:temporal xsi:type="abr:ABRperiode">MESOL</dct:temporal>
        <dct:temporal xsi:type="abr:ABRperiode">NEOV</dct:temporal>
        
    </ddm:dcmiMetadata>
    
    <ddm:profile>
        <dc:title></dc:title>
        <dc:description></dc:description>
        <dc:creator></dc:creator>
        <ddm:created>2012</ddm:created>
        <ddm:audience>D24000</ddm:audience>
        <ddm:accessRights>OPEN_ACCESS_FOR_REGISTERED_USERS</ddm:accessRights>
    </ddm:profile>

</ddm:DDM>
